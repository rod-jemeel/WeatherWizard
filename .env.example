# WeatherWizard Environment Variables

# API Keys
OPENWEATHER_API_KEY=your_openweather_api_key
OPENAI_API_KEY=your_openai_api_key

# Application Settings
FLASK_ENV=development  # Set to 'production' in deployment
SESSION_SECRET=your_session_secret  # Used for Flask's session encryption

# CORS Settings (only needed in deployment)
FRONTEND_URL=http://localhost:3000  # Update with your frontend URL in production
